{
  "definitions": {}, 
  "$schema": "http://json-schema.org/draft-07/schema#", 
  "type": "object", 
  "title": "New File Schema", 
  "required": [
    "file_data",
    "remote_file_name"
  ],
  "properties": {
    "file_data": {
      "$id": "#/properties/file_data", 
      "type": "string", 
      "title": "The data encoded in base64 of the file you want to upload",
      "examples": [
        "UmVjZWl2ZWQ6IGZyb20gaXAtMTkyLTE2O"
      ]
    },
    "remote_file_name": {
      "$id": "#/properties/remote_file_name", 
      "type": "string", 
      "title": "The name under which you want to store your file", 
      "examples": [
        "42.txt"
      ]
    }
  },
  "additionalProperties": false
}
